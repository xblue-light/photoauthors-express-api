{
  "name": "unsplash-express-api",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "devDependencies": {
    "ts-node": "10.7.0",
    "@types/node": "^16.11.10",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "ts-node-dev": "^2.0.0",
    "typeorm": "0.3.12",
    "winston": "^3.8.2"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "start:dev": "ts-node-dev src/index.ts",
    "migration:create": "npx typeorm-ts-node-commonjs migration:create ./src/migration/AdminUserNew",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d ./src/data-source.ts src/migrations/ChangeUserNameField",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/data-source.ts"
  }
}
